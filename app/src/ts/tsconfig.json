{
  "compilerOptions": {
    "target": "es2015",
    "moduleResolution": "node",
    "strict": true,
    "alwaysStrict": true,
    "esModuleInterop": true,
    "noEmitOnError": true,
    "noErrorTruncation": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noImplicitAny": true
  },
  "files": [
    "./main.ts"
  ]
}
